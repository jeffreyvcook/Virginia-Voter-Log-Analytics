= form_for @voter_transaction_log do |f|
  -if @voter_transaction_log.errors.any?
    #error_explanation
      %h2= "#{pluralize(@voter_transaction_log.errors.count, "error")} prohibited this voter_transaction_log from being saved:"
      %ul
        - @voter_transaction_log.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :origin
    = f.text_field :origin
  .field
    = f.label "Origin Unique"
    = f.text_field :origin_uniq
  .field
    = f.label :date
    = f.date_select :datime
  .field
    = f.label "Election Id"
    = f.text_field :election_id
